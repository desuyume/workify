    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "devDependencies": {
    "@workify/typescript-config": "workspace:*",
    "rimraf": "^6.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.3.0",
    "transliteration": "^2.6.0"
  }
